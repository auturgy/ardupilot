PA5 SPI1_SCK SPI1
PA6 SPI1_MISO SPI1
PA7 SPI1_MOSI SPI1
#PA8 PWM(6) TIM1_CH1 TIM1
#PA9 UART1_TX UART1 #CONSOLE
#PA10 UART1_RX UART1 #CONSOLE
PA11 OTG_FS_DM OTG1
PA12 OTG_FS_DP OTG1
