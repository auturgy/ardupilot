# hw definition file for processing by chibios_pins.py
# for MATEK F722-STD hardware.
@ thanks to betaflight for pin information (https://github.com/betaflight/betaflight/blob/master/src/main/target/MATEKF722/target.h)
# MCU class and specific type
MCU STM32F7xx STM32F722xx

define CONFIG_HAL_BOARD_SUBTYPE HAL_BOARD_SUBTYPE_CHIBIOS_MATEKF722

# board ID for firmware load
APJ_BOARD_ID 82

# crystal frequency, setup to use external oscillator
OSCILLATOR_HZ 8000000

define STM32_LSECLK   32768U
define STM32_LSEDRV   (3U << 3U)

define STM32_PLLSRC     STM32_PLLSRC_HSE
define STM32_PLLM_VALUE 8
define STM32_PLLN_VALUE 432
define STM32_PLLP_VALUE 2
define STM32_PLLQ_VALUE 9

RAM_SIZE_KB 256

FLASH_SIZE_KB 512
FLASH_RESERVE_START_KB 0

FLASH_SIZE_KB 512
FLASH_RESERVE_START_KB 0

# board voltage
STM32_VDD 330U

# only one I2C bus
I2C_ORDER I2C1

# order of UARTs (and USB)
UART_ORDER OTG1

#PA0 UART4_TX UART4 TIM5_CH1 TIM5
#PA1 UART4_RX UART4 TIM5_CH2 TIM5
#PA2 PWM(8) UART2_TX UART2 TIM5_CH3 TIM5
